 using System;

 class Date
 {
     private int day;
     private int month;
     private int year;

     public Date()
     {
         day = 1;
         month = 1;
         year = 2000;
     }

     public Date(int d, int m, int y)
     {
         SetYear(y);
         SetMonth(m);
         SetDay(d);
     }

     public void SetDay(int d)
     {
         int maxDays = DaysInMonth(month, year);
         if (d >= 1 && d <= maxDays)
             day = d;
         else
         {
             Console.WriteLine($"Некоректний день ({d}). Встановлено 1.");
             day = 1;
         }
     }

     public void SetMonth(int m)
     {
         if (m >= 1 && m <= 12)
             month = m;
         else
         {
             Console.WriteLine($"Некоректний місяць ({m}). Встановлено 1.");
             month = 1;
         }
     }

     public void SetYear(int y)
     {
         if (y >= 1)
             year = y;
         else
         {
             Console.WriteLine($"Некоректний рік ({y}). Встановлено 2000.");
             year = 2000;
         }
     }

     public int GetDay() => day;
     public int GetMonth() => month;
     public int GetYear() => year;

     private int DaysInMonth(int m, int y)
     {
         switch (m)
         {
             case 2:
                 return DateTime.IsLeapYear(y) ? 29 : 28;
             case 4:
             case 6:
             case 9:
             case 11:
                 return 30;
             default:
                 return 31;
         }
     }

     public void PrintLong()
     {
         string[] monthNames = {
         "", "січня", "лютого", "березня", "квітня", "травня", "червня",
         "липня", "серпня", "вересня", "жовтня", "листопада", "грудня"
     };

         Console.WriteLine($"{day} {monthNames[month]} {year} року");
     }

     public void PrintShort()
     {
         Console.WriteLine($"{day:00}.{month:00}.{year}");
     }
 }

 class Program
 {
     static void Main()
     {
         Console.InputEncoding = Encoding.Unicode;
         Console.OutputEncoding = Encoding.Unicode;
         Console.WriteLine("Тестування класу Date ");


         Date d1 = new Date();
         Console.Write("Дата за замовчуванням: ");
         d1.PrintLong();

        
         Date d2 = new Date(5, 1, 2019);
         Console.Write("Довгий формат: ");
         d2.PrintLong();
         Console.Write("Короткий формат: ");
         d2.PrintShort();

        
         Date d3 = new Date(35, 15, -100);
         Console.Write("\nПісля перевірки: ");
         d3.PrintShort();

         
         d3.SetYear(2025);
         d3.SetMonth(11);
         d3.SetDay(10);
         Console.Write("Оновлена дата: ");
         d3.PrintLong();
     }
 }



